{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:import/recommended",
    "plugin:node/recommended",
    "plugin:react/recommended",
    "plugin:security/recommended",
    "plugin:promise/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest"
  },
  "plugins": ["react", "security", "promise"],
  "settings": {
    "react": {
      "version": "latest"
    },
    "import/extensions": [".js", ".jsx"]
  },
  "rules": {
    "init-declarations": 0,
    "node/no-unpublished-require": 0,
    "node/shebang": 0,
    "indent": 0,
    "linebreak-style": [2, "unix"],
    "quotes": [2, "single"],
    "semi": [2, "always"],
    "no-var": 2,
    "no-unused-vars": 0,
    "no-path-concat": 2,
    "comma-dangle": 0,
    "array-bracket-newline": 2,
    "prefer-const": 2,
    "prefer-template": 2,
    "template-curly-spacing": [2, "never"],
    "no-use-before-define": [
      2,
      {
        "functions": true,
        "classes": true
      }
    ],
    "no-undef-init": 2,
    "eqeqeq": [2, "smart"],
    "no-useless-return": 2,
    "no-trailing-spaces": [
      1,
      {
        "ignoreComments": true
      }
    ],
    "node/no-unsupported-features/es-syntax": 0,
    "max-len": [
      2,
      {
        "code": 100,
        "ignoreStrings": true,
        "ignoreTemplateLiterals": true,
        "ignoreComments": true,
        "ignoreUrls": true
      }
    ],
    "prefer-numeric-literals": 2,
    "array-element-newline": [
      2,
      {
        "ArrayExpression": "consistent",
        "ArrayPattern": {
          "minItems": 2
        }
      }
    ],
    "generator-star-spacing": [2, "after"],
    "no-extra-boolean-cast": 0,
    "no-throw-literal": 2,
    "prefer-promise-reject-errors": 2,
    "space-before-function-paren": 0,
    "standard/computed-property-even-spacing": 0,
    "no-process-exit": 0
  }
}
